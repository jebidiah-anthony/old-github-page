<span class="nv">$dn</span><span class="o">=</span><span class="s2">"uid=ldapuser1,ou=People,dc=lightweight,dc=htb"</span><span class="p">;</span>

<span class="k">if</span> <span class="p">(</span><span class="nv">$bind</span><span class="o">=</span><span class="nb">ldap_bind</span><span class="p">(</span><span class="nv">$ds</span><span class="p">,</span> <span class="nv">$dn</span><span class="p">,</span> <span class="nv">$password</span><span class="p">))</span> <span class="p">{</span>
  <span class="k">echo</span><span class="p">(</span><span class="s2">"&lt;p&gt;&lt;i&gt;You may or may not see this page when you are banned. &lt;/i&gt;&lt;br&gt;&lt;br&gt;"</span><span class="p">);</span>
<span class="p">}</span> <span class="k">else</span> <span class="p">{</span>
  <span class="k">echo</span><span class="p">(</span><span class="s2">"Unable to bind to server.&lt;/br&gt;"</span><span class="p">);</span>
  <span class="k">echo</span><span class="p">(</span><span class="s2">"msg:'"</span><span class="o">.</span><span class="nb">ldap_error</span><span class="p">(</span><span class="nv">$ds</span><span class="p">)</span><span class="o">.</span><span class="s2">"'&lt;/br&gt;"</span><span class="o">.</span><span class="nb">ldap_errno</span><span class="p">(</span><span class="nv">$ds</span><span class="p">)</span><span class="o">.</span><span class="s2">""</span><span class="p">);</span>
  <span class="k">if</span> <span class="p">(</span><span class="nv">$bind</span><span class="o">=</span><span class="nb">ldap_bind</span><span class="p">(</span><span class="nv">$ds</span><span class="p">))</span> <span class="p">{</span>
    <span class="nv">$filter</span> <span class="o">=</span> <span class="s2">"(cn=*)"</span><span class="p">;</span>
    <span class="k">if</span> <span class="p">(</span><span class="o">!</span><span class="p">(</span><span class="nv">$search</span><span class="o">=@</span><span class="nb">ldap_search</span><span class="p">(</span><span class="nv">$ds</span><span class="p">,</span> <span class="nv">$ldapconfig</span><span class="p">[</span><span class="s1">'basedn'</span><span class="p">],</span> <span class="nv">$filter</span><span class="p">)))</span> <span class="p">{</span>
      <span class="k">echo</span><span class="p">(</span><span class="s2">"Unable to search ldap server&lt;br&gt;"</span><span class="p">);</span>
      <span class="k">echo</span><span class="p">(</span><span class="s2">"msg:'"</span><span class="o">.</span><span class="nb">ldap_error</span><span class="p">(</span><span class="nv">$ds</span><span class="p">)</span><span class="o">.</span><span class="s2">"'&lt;/br&gt;"</span><span class="p">);</span>
    <span class="p">}</span> <span class="k">else</span> <span class="p">{</span>
